@charset "utf-8";
/* CSS Document */
body { background-color:#f4f4f4;}

/*菜单*/

.menuwp { width:100%; padding-top:20px;}
.menuwp .menu { height:50px; line-height:50px; border:solid 1px #e9e9e9; border-bottom:0px; background-color:#ffffff;}
.menuwp .menu ul { padding-left:145px;}
.menuwp .menu ul li { float:left;padding:0px 18px;}
.menuwp .menu ul li a { padding:0px; height:50px; line-height:50px; display:block; padding:0px 5px; text-align:center; color:#4d4d4d; font-size:16px; text-align:center;}
.menuwp .menu ul li a:hover { text-decoration:none; color:#777;}
.menuwp .menu ul li.current {}
.menuwp .menu ul li.current a {color:#333; border-bottom:solid 3px #f4a51d; height:47px; line-height:50px;}

.userinfo { overflow:hidden; margin-bottom:20px; padding:20px; color:#ffffff;background-image:url("../images/user/menubg.jpg");background-position: center top;background-repeat:no-repeat; border:solid 1px #e9e9e9; border-top:0px;}
.userinfo .user_avatar {height: 97px;width: 97px; float:left;}
.userinfo .safe_rank{background:url(../images/user/security-icons.png) no-repeat;width:30px;height:30px;display:inline-block;_display:inline;zoom:1}

.userinfo .safe_rank.man{background-position:0 0}
.userinfo .safe_rank.man.light{background-position:0 -30px}
.userinfo .safe_rank.cellphone{background-position:-30px 0}
.userinfo .safe_rank.cellphone.light{background-position:-30px -30px}
.userinfo .safe_rank.mail{background-position:-60px 0}
.userinfo .safe_rank.mail.light{background-position:-60px -30px}
.userinfo .safe_rank.lock{background-position:-90px 0}
.userinfo .safe_rank.lock.light{background-position:-90px -30px}
.userinfo .safe_rank.verif{background-position:-120px 0}
.userinfo .safe_rank.verif.light{background-position:-120px -30px}

.userinfo .infomain { float:right; width:810px;font-size:16px;}
.userinfo .infomain .aer { width:100%; border-bottom:solid 1px #ffffff; line-height:48px; }
.userinfo .infomain .aer .logtime { font-size:13px; margin-left:15px;}

.userinfo .acc { line-height:48px;}
.userinfo .acc .name { float:left; padding-right:20px;}
.userinfo .acc .icolist { margin-top:8px;float:left; line-height:40px; padding-right:20px;}
.userinfo .acc .icolist li { float:left;}
.userinfo .acc .icolist li a { float:left;}
.userinfo .acc .safeh { float:left; padding-right:20px;}
.userinfo .acc .safeh span { font-weight:bold;}
.userinfo .acc .safeh span.safehHigh { color:#80cf12;}
.userinfo .acc .safeh span.safehInThe { color:#f6a41d;}
.userinfo .acc .safeh span.safehLow { color:#ff5234;}
.userinfo .acc .manager { float:left; padding-top:10px;}
.userinfo .acc .manager a {background-image:url("../images/user/setting-icons.png");background-position: center center;background-repeat:no-repeat; width:68px; height:27px; display:block; text-indent:-9999px;}

.left { width:150px; float:left;}
.left .tab_menu_box .tab_ul li{font-size: 16px;height:43px; line-height:43px;text-align:center; padding-right:10px;cursor:pointer;color: #9c9c9c;border-left:solid 3px #ffffff; border-bottom:solid 1px #e9e9e9;}
.left .tab_menu_box .tab_ul .current{background-color:#ffffff;color:#f4a51d; border-left:solid 3px #f4a51d;}

.left .leftmenu ul li{height:43px; line-height:43px;text-align:center; padding-right:10px;cursor:pointer;border-left:solid 3px #ffffff; border-bottom:solid 1px #e9e9e9; width:138px; float:left;}
.left .leftmenu ul li a {font-size: 16px;color: #9c9c9c; display:block;}
.left .leftmenu ul li.current{background-color:#ffffff;border-left:solid 3px #f4a51d;}
.left .leftmenu ul li.current a {color:#f4a51d; }


.rightmain { width:767px; padding:30px; float:right; border-left:solid 1px #e9e9e9; min-height:200px;}
.twocolumn {border:1px solid #e9e9e9;}


/*数据表格*/
.m-table{table-layout:fixed;width:100%;line-height:1.5; font-size:14px;}
.m-table th{padding:10px 5px;border:1px solid #e6e6e6;background-color:#eee;color:#434343;font-size:15px; text-align:center;}
.m-table td{padding:10px 5px;border:1px solid #e9e9e9; text-align:center;word-wrap: break-word;}
.m-table tbody tr:hover{background:#f9f9f9;}

.s-table{table-layout:fixed;width:100%;line-height:1.5; font-size:14px;}
.s-table th{padding:5px;border:1px solid #e6e6e6;background-color:#f4f4f4;color:#eee;font-size:14px; text-align:center;}
.s-table td{padding:5px;border:1px solid #e9e9e9; text-align:center;}
.s-table tbody tr:hover{background:#f9f9f9;}

.ico_tips {background:url("../images/user/ico_tips.png") no-repeat center center; display:inline-block; width:20px;height:20px;cursor: pointer;}

.captcha { display: inline-block; padding-left:15px; }
.captcha img { margin-top:7px; float:left; }
.captcha a.captchabtn { margin-left:10px; display:block; float:left; margin-top:4px; line-height:30px; background-image:url(../images/login/refresh.png); width:25px; height:28px;}



.pagetips { padding:30px; text-align:center;}
.pagetips p { font-size:16px;}
.pagetips .btn { text-align:center; padding-top:20px;}

/*弹窗公共*/
.popbox { min-width:400px;}
.popbox .btn { text-align:center;}

/*表单公用*/
.form li { margin-bottom:24px; position:relative;}
.form .input {width:398px;}
.form .sendphonecode { display:block;}
.form .sendphonecode .input { width:133px; float:left;}
.form .sendphonecode .ui-btn { width:133px; padding:0px; text-align:center; float:left; height:40px; line-height:40px; margin-left:5px;}


/*银行卡*/
.banklist { overflow:hidden;}
.banklist ul li {border: 3px solid #f6a41d;border-radius: 5px;color: #a5a5a5;cursor: pointer;display: inline;float: left;margin: 10px;overflow: hidden;position: relative;text-align: center;width: 250px; padding:15px 15px 15px 15px;}
.banklist ul li .card {width: 100%; font-size:18px; padding-top:10px;}



/*搜索*/
.searchbox { margin-bottom:15px;}
.searchbox .group {margin-bottom: 0;margin-right: 15px; position: relative; line-height:30px; margin-bottom:10px; height:30px; float:left;}
.searchbox .group .select { width:auto; font-size:13px;padding-left:3px;}
.searchbox .group .input { width:75px; font-size:13px; padding:5px 5px; line-height:18px;}
.searchbox .group .input.small { width:30px;}
.searchbox .group label.error { float:left; padding-left:45px; line-height:22px; color:#F00; position:absolute; left:0px; bottom:-17px; font-size:12px;}
.searchbox .ui-btn { min-width:60px; height:30px; line-height:30px;}
.searchbox .error { border:solid 1px #ff0000;}

/*翻页*/


.pager {text-align: center; padding-top:15px;}
.pager ul { float:right;}
.pager li {float: left;margin-left: 1px;height: 26px;line-height: 26px;text-align: center;}
.pager li a {padding: 0 9px;display: block;background-color: #e9e9e9;font-size: 12px;color: #848484;}
.pager li a:hover { background-color:#f4a51d; color:#ffffff;}
.pager .pager-current {color: #fff;background-color: #f4a51d;width:26px;}


/*用户首页*/



.user .function { background-color:#ffffff; padding:30px; margin-bottom:20px; border:1px solid #e9e9e9;}
.user .function .lie { width:100%; float:left; border-bottom:1px solid #e9e9e9; padding-bottom:20px; margin-bottom:20px;}
.user .function .lie .balan { width:300px; float:left; overflow:hidden;border-right:1px dashed #cdcdcd; padding:0px 10px 0px 0px;}
.user .function .lie .balan .tit { margin-bottom:10px;}
.user .function .lie .balan .money { line-height:30px; color:#f4a51d; font-size:36px;}
.user .function .lie .balan .money small { font-size:18px;}

.user .function .lie .balantwo { float:left; line-height:30px;border-right:1px dashed #cdcdcd; width:300px;padding:0px 0px 0px 30px;}
.user .function .lie .balantwo.last { padding-right:0px;}
.user .function .lie .balantwo .libox { width:100%;line-height:30px; }
.user .function .lie .balantwo .libox label { display:inline-block;width:80px;}


.user .function .btn { line-height:34px; padding-top:11px; float:left; text-align:center; width:275px;}
.user .function .btn .ui-btn { min-width:80px; line-height:28px; height:28px; margin:0px 10px;}


.user .function .balanthree { float:left; line-height:30px; border-right:1px dashed #cdcdcd; width:458px; }
.user .function .balanthree .libox { width:100%;}
.user .function .balanthree .libox label { display:inline-block; width:100px; position:relative;}
.user .function .balanthree .libox label .ico_tips { position:absolute; top:5px; right:30px;}

.user .function .balanfour { float:left; line-height:30px; width:428px; padding-left:30px;}
.user .function .balanfour .libox { width:100%;}
.user .function .balanfour .libox label { display:inline-block; width:135px;}



.user .products { margin-bottom:20px; overflow:hidden;}
.user .products h2 { height:30px; line-height:30px; margin-bottom:20px;}
.user .products h2 span { background-color:#FFF; float:left; display:inline-block; padding}
.user .products .btn { float:right;}
.user .products .btn .ui-btn { min-width:90px;line-height:28px; height:28px;}
.user .products .pbox {  position:relative; overflow:hidden; float:left; width:418px;background-color:#ffffff; padding:30px; margin-right:20px; border:1px solid #e9e9e9;}
.user .products .pbox.last { margin-right:0px;}
.user .products .pbox ul { overflow:hidden;}
.user .products .pbox ul li { line-height:30px; float:left; width:100%; margin-bottom:5px;}
.user .products .pbox ul li label { display:inline-block; width:100px; float:left;}
.user .products .pbox ul li .mantx { width:200px; display:inline-block; float:left;}
.user .products .pbox ul li.last { margin-right:0px;}

.user .IncomeCharts { padding:30px; margin-bottom:20px; overflow:hidden; background-color:#ffffff; border:1px solid #e9e9e9;}
.user .IncomeCharts h2 { height:30px; line-height:30px; margin-bottom:20px;}
.user .IncomeCharts h2 span { background-color:#FFF; float:left; display:inline-block; padding}
.user #IncomeCharts { width:100%; height:180px; float:right;}


.user .list { padding:30px; background-color:#FFF; margin-bottom:30px; border:1px solid #e9e9e9;}
.user .list h2 { height:30px; line-height:30px; margin-bottom:10px;}
.user .list h2 span { background-color:#FFF; float:left; display:inline-block;}
.user .list .btn {text-align: center;border-right-width: 1px;border-bottom-width: 1px;border-left-width: 1px;border-right-style: solid;border-bottom-style: solid;border-left-style: solid;border-right-color: #e9e9e9;border-bottom-color: #e9e9e9;border-left-color: #e9e9e9; padding:15px 0px;}
.user .list .btn a { color:#444444;}

/*充值*/
.recharge { padding:30px; background-color:#ffffff; margin-bottom:30px; border:1px solid #e9e9e9;}
.recharge .tab_menu_box {}
.recharge .tab_menu_box .tab_ul li { float:left;}
.recharge .tab_menu_box .tab_ul li a{font-weight: normal;font-size: 16px;float:left;height:40px; line-height:40px;min-width:90px;padding:0 22px;border:0px;text-align:center;cursor:pointer;color: #434343; margin-right:-1px; background-color:transparent;}
.recharge .tab_menu_box .tab_ul .current a{;border:0px; background-color:#f6a41d; color:#ffffff;}
.recharge .tab_content_box { padding:20px 0px 0px 0px; border-top:3px solid #f6a41d; margin-bottom:20px; background-color:#fff;}

.recharge .chargebankList { position:relative;}
.recharge .chargebankList dl dd{float:left;width:142px;height:40px;line-height:40px;border:1px solid #ddd;margin:0 17px;margin-bottom:20px;position:relative;display:inline;text-align:center}
.recharge .chargebankList dl dd:hover,
.recharge .chargebankList dl dd.current{border:1px solid #f6a41d}
.recharge .chargebankList dt{margin-bottom:5px}
.recharge .chargebankList dl dd label{display:block;width:142px;height:40px;overflow:hidden}
.recharge .chargebankList dl dd img{width:142px;height:40px;margin:0}
.recharge .chargebankList dl dd input{position:absolute;left:-20px;top:13px}

.recharge .rechargeform { text-align:center; width:345px; margin:0px auto; padding-top:20px;}
.recharge .rechargeform li { margin-bottom:24px;}
.recharge .rechargeform .formgroup { line-height:35px;}
.recharge .rechargeform .txttips { color:#999;}

.recharge .tipsbox { padding:20px; background-color: #f4f4f4; border:solid 1px #e9e9e9;}
.recharge .tipsbox dt { font-size:18px; padding-bottom:5px;}
.recharge .tipsbox dd { line-height:24px;}

.recharge .tipsmain { text-align:center;}
.recharge .tips {background-repeat: no-repeat;background-position: left 10px;padding:10px 10px 10px 90px;display:inline-block; text-align:left; margin:0px auto;}
.recharge .tips.right {background-image: url(../images/common/tips_right.png);}
.recharge .tips.right h2 { line-height:50px; margin:0px; color:#82c72e;}

.recharge .AuthenticationRecharge { text-align:center; width:460px; margin:0px auto; padding-top:20px;}
.recharge .AuthenticationRecharge li { margin-bottom:24px;}
.recharge .AuthenticationRecharge .formgroup { line-height:35px; vertical-align:middle; }
.recharge .AuthenticationRecharge .formgroup .title { width:85px;}
.recharge .AuthenticationRecharge .formgroup .input { width:250px; vertical-align:middle; }
.recharge .AuthenticationRecharge .errorbox { text-align:left; color:#ff0000; padding-left:85px;}

.PopChargebankListBox { padding:20px; width:715px; position:relative;}
.PopChargebankListBox #error1 { position:absolute; left:312px; bottom:-12px;}
.PopChargebankListBox .btn { text-align:center; line-height:34px;}
.PopChargebankListBox #BankCardNumberBtn { position:absolute; bottom:30px; left:20px;}

.PopChargebankNumber { padding:20px; width:455px; margin:0px auto;}
.PopChargebankNumber .input { width:350px;}
.PopChargebankNumber .btn { text-align:center; line-height:34px;}


/*提现*/
.withdrawals {padding:30px; background-color:#ffffff; margin-bottom:30px; border:1px solid #e9e9e9;}
.withdrawals h2 {}
.withdrawals .withdrawform { width:700px; margin-left:auto; margin-right:auto; margin-bottom:30px; margin-top:30px;}
.withdrawals .withdrawform li { margin-bottom:24px;}
.withdrawals .withdrawform li .select { width:203px;}
.withdrawals .withdrawform li #city_id-error { left:207px;}
.withdrawals .withdrawform li .selectfull { width:410px; margin-top:5px;}
.withdrawals .withdrawform .sendphonecode input { width:133px; float:left;}
.withdrawals .withdrawform .sendphonecode .ui-btn { width:133px; padding:0px; text-align:center; float:left; height:38px; line-height:38px; margin-left:5px;}
.withdrawals .withdrawform .title,.withdrawals .withdrawform .titlenol { width:120px; text-align:right; margin-right:10px;}
.withdrawals .withdrawform .btn { padding-left:130px;}
.withdrawals .withdrawform .btn .ui-btn { float:left; margin-right:5px;}
.withdrawals .withdrawform .inputtip { color:#999; padding-left:20px;}

.withdrawals .withdrawform .banklibox .titlenol { line-height:80px;}
.withdrawals .withdrawform .banklibox .text { height:56px; vertical-align:middle; border:2px solid #f6a41d; padding:10px;}
.withdrawals .withdrawform .banklibox .text span { display:inline-block;vertical-align:middle;}
.withdrawals .withdrawform .banklibox .text .img { line-height:0px;}
.withdrawals .withdrawform .banklibox .text .card { padding-left:15px;}

.withdrawals .withdrawform .Nobanklibox .titlenol { line-height:34px;}
.withdrawals .withdrawform .Nobanklibox .text { line-height:34px;}

.withdrawals .tipsbox {padding:20px; background-color: #f4f4f4; border:solid 1px #e9e9e9;}
.withdrawals .tipsbox dt { font-size:18px; padding-bottom:5px;}
.withdrawals .tipsbox dd { line-height:24px;}


/*银行卡管理*/
.bank { padding:30px; background-color:#ffffff; margin-bottom:30px;}
.bank .addform {}
.bank .addform { width:700px; margin-left:auto; margin-right:auto; margin-bottom:30px; margin-top:30px;}
.bank .addform li { margin-bottom:24px;}
.bank .addform li .tips { color:#cccccc; padding-left:20px;}
.bank .addform li input { width:300px;}
.bank .addform li select { width:322px;}
.bank .addform li.selecttwo select { width:158px;}
.bank .addform .sendphonecode input { width:161px; float:left;}
.bank .addform .sendphonecode .ui-btn { width:133px; padding:0px; text-align:center; float:left; height:38px; line-height:38px; margin-left:5px;}
.bank .addform .title,.bank .addform .titlenol { width:120px; text-align:right; margin-right:10px;}
.bank .addform .btn { padding-left:130px;}
.bank .addform .btn input { width:100px;}
.bank .addform .btn .ui-btn { float:left; margin-right:5px;}

/*交易记录*/
.transaction { background-color:#ffffff; margin-bottom:30px;}
.transactionview {padding-top:15px; padding-bottom:15px;}
.transactionview .status { padding:10px 15px 10px 15px;background-color: #dff0d8;color: #3c763d;border:solid 1px #d6e9c6;}
.transactionview h3 { margin-top:15px; margin-bottom:15px;}

/*充值弹出*/
.open_recharge { width:500px; padding:30px;}
.open_recharge h4 { margin-bottom:15px; margin-top:0px;}
.open_rechargelis { padding-bottom:8px; padding-top:8px;}
.open_rechargelis a { margin-right:8px; margin-left:8px; color:#39a1ea;}
.open_rechargelis a:hover { text-decoration:underline;}
.open_rechargelis .recharge_ico_success {font-weight: bold;background-image: url(../images/icon/recharge_ico_success.png);background-repeat: no-repeat;background-position: left center;padding-left: 20px;}
.open_rechargelis .recharge_ico_failure {font-weight: bold;background-image: url(../images/icon/recharge_ico_failure.png);background-repeat: no-repeat;background-position: left center;padding-left: 20px;}


/*消息中心*/

.message { margin-bottom:30px; background-color:#ffffff; border:1px solid #e9e9e9;}

.message #notif .inbox { overflow:hidden; width:100%;}
.message #notif .inbox a{ color:#444;}
.message #notif .unread { font-weight:bold;}
.message #notif .inbaddressor { width:20%; float:left;}
.message #notif .inbtitle {width:56%; float:left;}
.message #notif .inbtime {width:20%; float:left;}

.message #notif .inbox_title { overflow:hidden; line-height:35px; font-weight:bold; font-size:16px;padding-left:55px; }
.message #notif .inbox_no { border:solid 1px #f4f4f4; overflow:hidden; line-height:35px; margin-bottom:5px; background-color:#f9f9f9; text-align:center;}
.message #notif .inbox_content { border:solid 1px #f4f4f4; overflow:hidden; line-height:35px; margin-bottom:5px; padding-left:55px; background-color:#f9f9f9;}

.message .message_detail { display:none; padding-top:15px;}
.message .message_detail .message_remind {background-image: url(../images/user/icon-fanhui.png);background-repeat: no-repeat;background-position: left center;padding-left: 31px;font-size:16px;}
.message .message_detail .message_remind:hover { text-decoration: underline;}

.message .message_detail .message_toptit { overflow:hidden; padding-top:15px;}
.message .message_detail .message_toptit .title{ font-size:16px; font-weight:bold;}
.message .message_detail .message_detail_content { padding-top:15px; padding-bottom:15px; padding-left:25px;word-wrap: break-word;}
.message .message_detail .message_bottom { padding-top:10px; border-top: 1px solid #e6e6e6; overflow:hidden;}


/*安全中心*/
.safe .level { width:767px; height:23px; line-height:23px; position:relative;background-image: url(../images/user/level-bg.png);background-repeat: no-repeat;background-position: left center; margin-top:20px;}
.safe .LevelGuide {background-image: url(../images/user/level-guide.png);background-repeat: no-repeat;background-position: left center; position:absolute; top:-24px; padding-left:16px; color:#f4a51d; margin-left:-7px; width:30px;}

.safe .LevelGuide.safe20 { left:20%;}
.safe .LevelGuide.safe40 { left:40%;}
.safe .LevelGuide.safe60 { left:60%;}
.safe .LevelGuide.safe80 { left:80%;}
.safe .LevelGuide.safe100 { left:95.5%;padding-left:0px; padding-right:16px;background-position: right center;}

.safe .levelbox {background-image: url(../images/user/level.png);background-repeat: no-repeat;background-position: left center; display:block; height:23px;}
.safe .level .dangertext,.safe .level .warningtext,.safe .level .successtext { position:absolute; bottom:-24px;}
.safe .level .dangertext { left:250px;}
.safe .level .warningtext { left:506px;}
.safe .level .successtext { right:0px;}


.safe { background-color:#ffffff; margin-bottom:30px;  border:1px solid #e9e9e9;}
.safe .group { padding-top:60px;}
.safe .group .line { overflow:hidden; line-height:40px; padding-top:8px; padding-bottom:8px;}
.safe .group .line.odd { background-color:#f4f4f4;}
.safe .group .line.last { border-bottom:0px;}

.safe .group .line .title { float:left; width:170px; margin-left:30px; padding-left:32px;background-image: url(../images/user/group-icons.png);background-repeat: no-repeat;background-position: left top;}
.safe .group .line .title.iuser {background-position: left 0px;}
.safe .group .line .title.ipass {background-position: left -40px;}
.safe .group .line .title.iidc {background-position: left -80px;}
.safe .group .line .title.imobile {background-position: left -120px;}
.safe .group .line .title.iemail {background-position: left -160px;}

.safe .group .line .sub { float:left; width:270px; text-align:center;}
.safe .group .line .sub .dangertext { color:#ff5134;}
.safe .group .line .sub .warningtext { color:#f4a51d;}
.safe .group .line .sub .successtext { color:#81cf13;}
.safe .group .line .btn { float:right; padding-right:30px; height:34px; line-height:34px;}
.safe .group .line .btn .ui-btn { height:32px; line-height:32px; border:solid 1px #f4a51d; background-color:#ffffff; color:#f4a51d; font-size:15px; padding-left:20px; min-width:65px;background-image: url(../images/user/safe_btn.png);background-repeat: no-repeat;background-position: 14px top;}
.safe .group .line .btn .ui-btn:hover { background-color:#f4a51d; color:#ffffff;}

.safe .group .line .btn .ui-btn.safeBtnModify {background-position: 14px 0px;}
.safe .group .line .btn .ui-btn.safeBtnModify:hover {background-position: 14px -34px;}
.safe .group .line .btn .ui-btn.safeBtnBack {background-position: 14px -68px;}
.safe .group .line .btn .ui-btn.safeBtnBack:hover {background-position: 14px -102px;}
.safe .group .line .btn .ui-btn.safeBtnSetup {background-position: 14px -136px;}
