/*计算器*/
.calculator_icon { width:20px; height:27px; background-image:url(../images/loan/calculator_icon.png); background-repeat:no-repeat; background-position:center center; position:absolute; top:30px; right:30px; z-index:999;}
.calculator_icon:hover {opacity: 0.8;}
.calculator { width:310px; padding:30px;}
.calculator .CalculatorForm li { margin-bottom:24px;}
.calculator .CalculatorForm li .input { width:160px; padding:6px; height:15px;}
.calculator .CalculatorForm .radiobox label { display:inline-block; padding-right:15px;}
.calculator .CalculatorForm .btn { padding-left:95px;}
.calculator .CalculatorForm .btn input { width:100px;}
.calculator .CalculatorForm .title {width:85px; text-align:right; margin-right:10px; line-height:24px;}
.calculator .CalculatorForm .titlenol { width:85px; text-align:right; margin-right:10px;}

.calculator .CalculatorResult { border-top: solid 1px #e9e9e9; padding:10px 0px 0px 0px; height:76px;}
.calculator .CalculatorResult h3 { margin-bottom:5px; font-size:16px;}
.calculator .CalculatorResult p { margin:0px;}
.calculator .CalculatorResult span { color:#f4a51d; font-weight:bold;}






