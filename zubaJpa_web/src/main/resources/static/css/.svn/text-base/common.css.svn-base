@charset "utf-8";
/* reset */
html,body,h1,h2,h3,h4,h5,h6,div,dl,dt,dd,ul,ol,li,p,blockquote,pre,hr,figure,table,caption,th,td,form,fieldset,legend,input,button,textarea,menu{margin:0;padding:0;}
header,footer,section,article,aside,nav,hgroup,address,figure,figcaption,menu,details{display:block;}
table{border-collapse:collapse;border-spacing:0;}
caption,th{text-align:left;font-weight:normal;}
html,body,fieldset,img,iframe,abbr{border:0;}
i,cite,em,var,address,dfn{font-style:normal;}
[hidefocus],summary{outline:0;}
li{list-style:none;}
h1,h2,h3,h4,h5,h6,small{font-size:100%;}
sup,sub{font-size:83%;}
pre,code,kbd,samp{font-family:inherit;}
q:before,q:after{content:none;}
textarea{overflow:auto;resize:none;}
label,summary{cursor:default;}
a,button{cursor:pointer;}
h1,h2,h3,h4,h5,h6,strong,b{font-weight:bold;}
del,ins,u,s,a,a:hover{text-decoration:none;}
body,textarea,input,button,select,keygen,legend{font:14px/1.5 "微软雅黑",YaHei,tahoma,arial,"Hiragino Sans GB",宋体;color:#444;outline:0;}
a{color:#f4a51d;outline:none;blr:expression(this.onFocus=this.blur());}
a:hover {color: #e59b1c;}

body{min-height:100%;position:relative;-webkit-text-size-adjust:none; min-width: 1000px;}
html {height: 100%;-webkit-font-smoothing: antialiased;-webkit-text-size-adjust: none;}
html{overflow-y:scroll}
input,button{*overflow:visible;}

p { line-height:22px; margin-bottom:10px;}
h1 { font-size:28px; margin-bottom:15px;}
h2 { font-size:22px; margin-bottom:15px;}
h3 { font-size:20px;}
h4 { font-size:16px; margin-bottom:5px; margin-top:5px;}

.fl { float:left;}
.fr { float:right;}
.hide { display:none;}
.clearfix:after {content:"\200B";display:block;height:0;clear:both;}
.clearfix {*zoom:1;}
.red { color:#ff0000;}
/*按钮基类*/
.ui-btn{user-select:none;-moz-user-select: none;-webkit-user-select: none; -ms-user-select: none;display:inline-block;*display:inline;*zoom:1;background-color:#f4a51d;color:#fff;text-align:center; min-width:100px;font-size:13px;height:34px;line-height:34px;cursor:pointer;border-radius:4px;border:none;width:auto;text-decoration:none;vertical-align:middle;}
.ui-btn:hover{text-decoration:none;color:#fff;background-color:#e59b1c;}
.ui-btn:active{background-color:#dc951b;box-shadow:inset 2px 2px 6px #d38f19;}
/*按钮扩展*/
.ui-btn-grew{background-color:#c4c4c4;}
.ui-btn-grew:hover{background-color:#bbbbbb;}
.ui-btn-grew:active{background-color:#b3b3b3;box-shadow:inset 2px 2px 6px #adadad;}
.ui-btn-red{background-color:#ff5f26;}
.ui-btn-red:hover{background-color:#f05a25;}
.ui-btn-red:active{background-color:#e85825;box-shadow:inset 2px 2px 6px #da5120;}

/*按钮大号*/
.ui-btn-big{height:42px;line-height:42px;padding:0 30px;font-size:16px;}
/*添加置灰样式*/
.ui-btn.disabled,
.ui-btn.disabled:hover,
.ui-btn.disabled:active,
.ui-btn[disabled],
.ui-btn[disabled]:hover,
.ui-btn[disabled]:active{cursor:default;color:#fff;background-color:#ddd;text-decoration:none;outline:none;box-shadow:none;}

/*表单*/
.input {width:180px;padding:10px;height:16px;border:1px solid #D9D9D9;border-top-color:#c0c0c0;line-height:17px;font-size:14px;color:#777;background:#fff;}
.select{margin-right:5px;width:250px;border:1px solid #b2b2b2;padding:3px 0px 3px 7px;font-size:14px;font-family:"microsoft yahei"; margin:0px;-webkit-appearance:none;
        appearance:none;  }
.select option{ line-height:24px;}
.radio { width:20px;}
.textarea {border: 1px solid #b2b2b2;height: 100px;overflow-x: hidden;overflow-y: auto;padding: 9px 9px;width: 585px;line-height: 18px;outline: medium none;resize: none;}
.input_required { color:#F00; margin-right:5px;}

.formgroup:after {content:"\200B";display:block;height:0;clear:both;}
.formgroup {*zoom:1;}
.formgroup .title { float:left; line-height:36px;}
.formgroup .titlenol { float:left;}
.formgroup .text { float:left;}
.formgroup .forminput { position:relative; display:inline-block; float:left;}
.formgroup .forminput select { margin-top:4px;}
.formgroup .formradio { position:relative; display:inline-block; float:left;}
.formgroup input.error { border:1px solid #dc4d3b;}
.formgroup label.error {text-align: left !important; background-image:url("../images/common/validate.png");background-position: 0 5px;background-repeat:no-repeat;padding-left: 20px;height: 24px;line-height: 24px;width: auto !important;display: block;color: #dc4d3b;font-size: 12px;position:absolute; left:0px; bottom:-24px;}

input[disabled], select[disabled], textarea[disabled], input[readonly], select[readonly], textarea[readonly] {background-color:#eee;}
/*表格*/


/*布局*/
.container {width:100%;}
.innerbox {width:980px;margin: 0px auto;padding-left:10px; padding-right:10px;}

/*头部*/
