.header .loginin .popmain a:hover { background-color:#f4a51d; color:#fff;}
.header .loginin .popmain .arrow,.site-neck-indoor .arrow {display:block;position:absolute;z-index:974;top:-6px;right:7px;width:0;height:0;font-size:0;line-height:0;border-color:transparent transparent #adadad;border-width:0 6px 6px;border-style:dashed dashed solid;}
.header .loginin .popmain .arrow-over,.site-neck-indoor .arrow-over {display:block;position:absolute;z-index:974;top:-5px;right:7px;width:0;height:0;font-size:0;line-height:0;border-color:transparent transparent #fff;border-width:0 6px 6px;border-style:dashed dashed solid;}
