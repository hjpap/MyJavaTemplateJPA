.header .loginin li a .arrow-down {width:0;height:0;border-left:4px solid transparent;border-right:4px solid transparent;border-top:4px solid #555; line-height:0px !important;position: absolute;right: 0px;top: 14px;z-index: 1;}
.header .loginin li.current .arrow-down { 
	-webkit-transform:rotate(180deg);-moz-transform:rotate(180deg);-o-transform:rotate(180deg);transform:rotate(180deg);-webkit-transition:all .25s ease 0s;-moz-transition:all .25s ease 0s;-o-transition:all .25s ease 0s;transition:all .25s ease 0s
}

.header .loginin .popmain{position:absolute;top:30px;right:0px;z-index:9999;width:100px;padding:5px;display:none;border:1px solid #ccc;background:#fff;-webkit-box-shadow: 0 0 2px #b6b3ac;-moz-box-shadow: 0 0 2px #b6b3ac;box-shadow: 0 0 2px #b6b3ac;}
