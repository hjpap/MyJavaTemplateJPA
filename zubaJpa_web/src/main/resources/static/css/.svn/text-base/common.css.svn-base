.header .loginbox li { float:left; margin-right:10px;}
.header .loginbox a { display:block; padding:0px 10px; border:solid 1px #ccc; line-height:30px; color:#666666;}
.header .loginbox a:hover { border:solid 1px #ddd; color:#666;text-decoration:none;color:#444444;}
